    "assertion-error": "^1.0.2",
    "babel-runtime": "^6.18.0",
    "deep-eql": "^2.0.1",
    "lodash.merge": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "should-format": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cupools/checkin.git"
  },
  "keywords": [
    "validator",
